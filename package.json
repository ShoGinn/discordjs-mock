{
  "name": "@shoginn/discordjs-mock",
  "version": "1.0.7",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "author": "@ShoGinn",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./package.json": {
      "import": "./package.json",
      "require": "./package.json"
    }
  },
  "devDependencies": {
    "@tsconfig/strictest": "^2.0.5",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.7",
    "@types/node": "^22.5.2",
    "@typescript-eslint/eslint-plugin": "^8.4.0",
    "eslint": "^9.9.1",
    "eslint-config-prettier": "~9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^28.8.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unicorn": "^55.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.5",
    "ts-jest-resolver": "^2.0.1",
    "ts-node": "^10.9.2",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.4.0"
  },
  "scripts": {
    "prepublishOnly": "pnpm build",
    "build": "tsup index.ts --target node18 --format cjs,esm --dts --sourcemap",
    "build-watch": "pnpm build -- --watch",
    "clean": "rm -rf dist coverage",
    "test": "pnpm jest --coverage",
    "test:watch": "pnpm jest --watch",
    "format": "prettier --ignore-path .gitignore --config .prettierrc.cjs --write ./src",
    "format:check": "prettier --ignore-path .gitignore --config .prettierrc.cjs --check ./src",
    "lint": "eslint ./src && tsc --noEmit",
    "lint:fix": "eslint ./src --fix"
  },
  "peerDependencies": {
    "discord.js": "^14.16.0"
  },
  "dependencies": {
    "@sapphire/snowflake": "^3.5.3",
    "discord.js": "^14.16.1",
    "lodash": "^4.17.21"
  }
}